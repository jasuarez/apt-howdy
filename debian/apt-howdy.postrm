#!/bin/sh
# postinst script for apt-howdy
#
# see: dh_installdeb(1)

set -e

# Remove apt hook
rm -f /etc/apt/apt.conf.d/20apt-howdy

# Remove database dir
rm -rf /var/lib/apt-howdy

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
