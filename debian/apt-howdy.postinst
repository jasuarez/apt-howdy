#!/bin/sh
# postinst script for apt-howdy
#
# see: dh_installdeb(1)

set -e

# Run first time

/usr/lib/apt-howdy-check

# Register alarm for future executions

/usr/lib/apt-howdy-check register

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
